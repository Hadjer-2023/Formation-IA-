    "from flask import Flask, render_template\n",
    "app = Flask(__name__)\n",
    "\n",
    "@app.route(\"/\")\n",
    "def hello():\n",
    "    return render_template(\"home.html\", message = \"Hello World!\")\n",
    "\n",
    "if __name__ == \"__main__\":\n",
    "    app.run()"
